{
  "name": "load-available-actions",
  "version": "0.0.1",
  "private": false,
  "description": "GitHub Action to load all available actions for an organization or user",
  "main": "dist/main.js",
  "scripts": {
    "esbuild": "esbuild src/main.ts --bundle --outdir=dist --platform=node --external:vscode --external:@vscode/test-electron --external:jest",
    "build": "npm run esbuild",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "npm run esbuild",
    "test": "jest",
    "all": "npm run esbuild && npm run lint &&  npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajbos/github-action-load-available-actions.git"
  },
  "keywords": [
    "GitHub Actions",
    "load available actions from GitHub organization"
  ],
  "author": "@rajbos",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "html-entities": "^2.6.0",
    "moment": "^2.30.1",
    "octokit": "^5.0.3",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@typescript-eslint/parser": "^8.35.1",
    "@vercel/ncc": "^0.38.3",
    "ansi-regex": ">=6.1.0",
    "dotenv": "^17.0.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.6.2",
    "tmpl": ">=1.0.5",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
